---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>{{ site.name }}</title>
  <link href="http://overhead.github.io/RecipeSite/"/>
  <link type="application/atom+xml" rel="self" href="http://overhead.github.io/RecipeSite/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://overhead.github.io/RecipeSite/</id>
  <author>
    <name>Emil and Jon Martin</name>
    <email>emipir-1@student.ltu.se</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://overhead.github.io/RecipeSite{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://overhead.github.io/RecipeSite{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Emil and Jon Martin</name>
      <uri>http://overhead.github.io/RecipeSite/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
